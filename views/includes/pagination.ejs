<section class="pagination">
  <% if(curentPage !== 1 && previousPage !== 1) { %>
  <a href="?page=1">1</a>
  <% } %> <% if(hasPreviousPage) { %>
  <a href="?page=<%= previousPage %>"><%= previousPage %></a>
  <% } %>
  <a href="?page=<%= curentPage %>" class="active"><%= curentPage %></a>
  <% if(hasNextPage) { %>
  <a href="?page=<%= nextPage %>"><%= nextPage %></a>
  <% } %> <%if(lastPage !== curentPage && nextPage !== lastPage){%>
  <a href="?page=<%= lastPage %>"><%= lastPage %></a>
  <% } %>
</section>
